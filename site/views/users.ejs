<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link rel="stylesheet" href="/stylesheets/registo.css">
    <link href="https://fonts.googleapis.com/css?family=Passion+One:700&display=swap" rel="stylesheet">

    <title>Users</title>
</head>
<body>
    <div class="container-fluid">
      
            <div class="container">
                <div class="row" >
                    <div class="col 4"><img src="/images/logo-retro.jpg" alt="" class="rounded-circle" width="50%"> </div>
                    <div class="col-8"><h1 id="titulo"> RetroFutbolStore</h1></div>
                </div>
        
                <%- include("./partials/nuevomenu") %>
                
                <!-- Tab panes -->
                
                </div>
           
          
        
      

            
            <!-- Tab panes -->
            <div class="tab-content">
              <div class="tab-pane container active" "id"="home">...</div>
              <div class="tab-pane container fade" "id"="menu1">...</div>
              <div class="tab-pane container fade" "id"="menu2">...</div>
              <div class="tab-pane container fade" "id"="menu2">...</div>
            </div>
    
           
            <h1>Bienvenido:</h1>
            <% for(user of users) { %>
                <div class="col-4 ">
                    <div class="card mb-4 shadow-sm ">
                        
                        <div class="card-body">
                            <img src="/images/users<%= user.avatar %>" width="100%">
                            <h5 style="text-align: center;" class="card-title" >Nombre: <%= user.name %></h5>
                            
                            <p style="text-align: center;" class="card-text">Apellido: <%= user.lastname %></p>
                        </div>
                    </div>
                </div>
                <% } %>
    
    
            
    
            
        </div>
      
        
        <footer>
            <%- include('partials/footer'); %>
        </footer>
    </div>

  </div> 

<script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
</body>
</html>